                  },
                  "title": "Total Cost by Linked Account",
                  "description": null,
                  "definition": {
                    "gauge": {
                      "query": {
                        "dataprime": {
                          "dataprimeQuery": {
                            "text": "source logs\r\n  | filter $d.Granularity == 'Daily' && $d.LinkedAccount != 'N/A' && $d.ScanID != 'N/A'&& LinkedAccount != null\r\n  | groupby $d.LinkedAccount aggregate sum($d.CostUsd:number/1000000) as total_cost\r\n  | sortby total_cost desc\r\n  | limit 1"
                          },
                          "filters": [],
                          "timeFrame": null
