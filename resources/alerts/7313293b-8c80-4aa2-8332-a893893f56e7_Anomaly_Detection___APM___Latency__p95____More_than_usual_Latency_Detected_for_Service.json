      "priority": "ALERT_DEF_PRIORITY_P3",
      "type": "ALERT_DEF_TYPE_METRIC_ANOMALY",
      "groupByKeys": [],
      "incidentsSettings": {
        "notifyOn": "NOTIFY_ON_TRIGGERED_ONLY_UNSPECIFIED",
        "minutes": 10
      },
      "notificationGroup": {
        "groupByKeys": [],
        "webhooks": [],
        "destinations": []
      },
      "entityLabels": {
        "anomaly_detection": "core"
      },
      "phantomMode": false,
      "deleted": false,
      "dataSources": [],
      "metricAnomaly": {
        "metricFilter": {
          "promql": "(histogram_quantile(0.95, sum(increase(duration_ms_bucket{span_kind=~\"SPAN_KIND_SERVER|SPAN_KIND_CONSUMER\", service_name=~\".*\"})) by (service_name,le))  * 1000)[30s] or on () vector(0)"
        },
        "rules": [
          {
            "condition": {
              "threshold": 1,
              "forOverPct": 0,
              "ofTheLast": {
                "metricTimeWindowSpecificValue": "METRIC_TIME_WINDOW_VALUE_MINUTES_10"
