                          "dataprimeQuery": {
                            "text": "source logs\n| filter contains(key, '/var/log/remote/') && contains(key, 'dhcp') && contains(key, 'DHCPOFFER')\n| extract message into helper_data using regexp(e=/via (?<helper>\\d+\\.\\d+\\.\\d+\\.\\d+)/)\n| extract message into dhcp_data using regexp(e=/DHCPOFFER on (?<OfferedAddress>\\d+\\.\\d+\\.\\d+\\.\\d+) to (?<MACAddress>[0-9a-f]{2}:[0-9a-f]{2}:[0-9a-f]{2}:[0-9a-f]{2}:[0-9a-f]{2}:[0-9a-f]{2})\\s+\\((?<Hostname>\\S+)\\)/)\n| groupby dhcp_data.OfferedAddress aggregate collect(dhcp_data.Hostname) as Hostnames, collect(dhcp_data.MACAddress) as MACAddresses"
                          },
                          "filters": [],
                          "timeFrame": null
                        }
