                            "dataprime": {
                              "dataprimeQuery": {
                                "text": "source logs last 7d\n| filter $l.subsystemname == 'product-catalog' && productId_enriched.category!=''\n| filter productId_enriched.price_in_currency:num > 0\n| create Trans_usd from \n  case_equals {\n        productId_enriched.currency,\n        'USD'   -> productId_enriched.price_in_currency:num * 1,\n        'EUR'   -> productId_enriched.price_in_currency:num * 1.04,\n        'JPY'   -> productId_enriched.price_in_currency:num * 0.0066,\n        'CAD'   -> productId_enriched.price_in_currency:num * 0.69}\n| groupby $m.timestamp/1h as Time, productId_enriched.category as Categories agg \n    count() as Transactions, \n    `${round(sum(Trans_usd),2)}` as Total_Revenue,\n    `${round(avg(Trans_usd),2)}` as Avg_Revenue\n| orderby Time"
