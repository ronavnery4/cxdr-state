                  "title": "Requests relative to 1 day ago",
                  "description": null,
                  "definition": {
                    "dataTable": {
                      "query": {
                        "dataprime": {
                          "dataprimeQuery": {
                            "text": "source logs between now() - 7d and now() | groupby $m.timestamp / 1d as ts aggregate sum(http_resp_bytes) as today_requests\n| join (\n    source logs between now() - 8d and now() - 1d | groupby $m.timestamp / 1d as ts aggregate sum(http_resp_bytes) as yesterday_requests\n) on left=>ts == right=>ts + 1d into comparison\n| create percentage_difference from (comparison.yesterday_requests / today_requests) * 100 - 100\n| sortby comparison.ts\n| choose comparison.ts.formatTimestamp('%F') as Date, today_requests as Today, comparison.yesterday_requests as Yesterday,  percentage_difference as PercentChange"
                          },
                          "filters": [],
                          "timeFrame": null
                        }
                      },
