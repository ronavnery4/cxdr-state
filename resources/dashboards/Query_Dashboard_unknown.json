                  "title": "Show 500 Error log percentage",
                  "description": null,
                  "definition": {
                    "dataTable": {
                      "query": {
                        "dataprime": {
                          "dataprimeQuery": {
                            "text": "source logs\n| groupby $l.subsystemname aggregate count() as total_logs, count_if($d.http_resp_status==500) as error_logs\n| create $d.error_percentage from (error_logs / total_logs) * 100\n| orderby $d.error_percentage desc"
                          },
                          "filters": [],
                          "timeFrame": null
