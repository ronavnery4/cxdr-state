                    "dataTable": {
                      "query": {
                        "dataprime": {
                          "dataprimeQuery": {
                            "text": "source logs | lucene 'operationName:\"sign\\-in activity\" AND _exists_:\"properties.status_obj.failureReason\"' | countby $d.properties.signInIdentifier as $d.User_Name, $d.properties.status_obj.failureReason as $d.Failure_Reason | filter $d.User_Name != null | sortby $d._count desc"
                          },
                          "filters": [],
                          "timeFrame": null
                        }
                      },
